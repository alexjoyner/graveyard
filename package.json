{
  "name": "ro-utils",
  "version": "0.0.2",
  "description": "A library set of quick utilities",
  "main": "index.js",
  "scripts": {
    "start": "npm index.js",
    "watch": "nodemon index.js",
    "test": "mocha **/*.spec.js --reporter spec --compilers js:babel-core/register  --require ignore-styles ./client/test_helper.js",
    "test:watch": "PORT=2080 npm run test -- --watch",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec **/*.spec.js  --compilers js:babel-core/register",
    "cover:with_open": "npm run cover && open ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://alexjoyner@github.com/alexjoyner/ro-utils.git"
  },
  "keywords": [
    "utilities"
  ],
  "author": "Alex Joyner <alex@voicir.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alexjoyner/ro-utils/issues"
  },
  "homepage": "https://github.com/alexjoyner/ro-utils#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.6.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.1.18",
    "babel-preset-stage-2": "^6.13.0",
    "beeper": "^1.1.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "coveralls": "^2.13.1",
    "ignore-styles": "^5.0.1",
    "istanbul": "^1.1.0-alpha.1",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^3.3.0",
    "react-addons-test-utils": "^15.5.1",
    "rewire": "^2.5.2",
    "sinon": "^2.2.0"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "morgan": "^1.8.1",
    "node-cache": "^4.1.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0"
  }
}
