{
  "name": "your-project-name",
  "version": "v0.0.1",
  "scripts": {
    "start": "concurrently --kill-others 'npm run server:dev:watch' 'npm run webpack:dev'",
    "build": "NODE_ENV='production' webpack -p",
    "production": "pm2 start process.yml --env env_production && pm2 logs",
    "server:dev": "babel-node ./server/server.js --presets es2015,stage-2",
    "server:dev:watch": "nodemon ./server/server.js --exec babel-node --presets es2015,stage-2",
    "webpack:dev": "webpack-dev-server -d --progress --profile --colors --watch --display-error-details --content-base ./dist_client --inline --hot ",
    "test": "mocha $(find ./client ./server -path '*spec*') --full-trace --compilers js:babel-core/register --require ignore-styles ./client/_startup/test_helper.js",
    "cover": "PORT=2081 node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec $(find ./client ./server -path '*spec*')  --compilers js:babel-core/register ",
    "test:watch": "PORT=2080 npm run test -- --watch ",
    "cover:with_open": "npm runt cover && open ./coverage/lcov-report/index.html"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lodash": "^3.10.1",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-cache": "^3.2.1",
    "q": "^1.4.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "redux": "^3.0.4",
    "redux-form": "^6.0.5",
    "redux-promise": "^0.5.3",
    "ro-utils": "0.0.1"
  },
