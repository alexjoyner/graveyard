{
  "name": "metatruth",
  "version": "1.0.0-Alpha.11.7",
  "scripts": {
    "start": "concurrently --kill-others 'npm run server:dev' 'npm run webpack:dev'",
    "build": "NODE_ENV='production' webpack -p",
    "production": "pm2 start process.yml --env env_production && pm2 logs",
    "server:dev": "nodemon server.js",
    "webpack:dev": "webpack-dev-server -d --progress --profile --colors --watch --display-error-details --content-base ./dist_client --inline --hot ",
    "test": "mocha $(find src -path '*spec*') --full-trace --compilers js:babel-core/register --require ignore-styles ./src/js/test_helper.js",
    "test:watch": "npm run test -- --watch"
  },
